generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Menu {
  id Int @id @default(autoincrement())
  name String
  posts Post[]
}

model Post {
  id Int @id @default(autoincrement())
  menuId Int
  menu Menu @relation(fields: [menuId],references:[id])
  weight Float
  reps Int
  sets Int
  date DateTime @default(now())
}


